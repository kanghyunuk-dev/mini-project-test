public class DetailPage {
	public static void showDetail(int boardIndex) {
		Scanner scan = new Scanner(System.in);
		DB db = new DB();
		String boardNum = null;
        System.out.println("게시글 번호 " + boardIndex + "의 내용을 출력합니다.");
        db.detail(boardNum);
    }
	
}
